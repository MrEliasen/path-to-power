{
  "name": "path-to-power-client",
  "version": "0.1.0",
  "license": "CC-BY-3.0",
  "description": "A simple MUD inspired, browser-based multiplayer game.",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --inline --config webpack.development.js",
    "lint": "eslint src",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --progress --config webpack.production.js",
    "postinstall": "cp -n src/config.sample.js src/config.js && echo 'Config file created at src/config.js. Remember to edit it!'"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrEliasen/path-to-power-client.git"
  },
  "author": "MrEliasen (@markeliasen)",
  "bugs": {
    "url": "https://github.com/MrEliasen/path-to-power-client/issues"
  },
  "homepage": "https://www.pathtopower.online",
  "dependencies": {
    "jsonp": "^0.2.1",
    "material-ui": "^0.20.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.4.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
