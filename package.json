{
    "name": "path-to-power-server",
    "version": "0.1.0",
    "description": "The server for a simple MUD inspired browser-based multiplayer game.",
    "scripts": {
        "start": "nodemon -L server.js --inspect --exec babel-node --presets env,stage-0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MrEliasen/path-to-power-server.git"
    },
    "author": "SirMrE (@markeliasen)",
    "bugs": {
        "url": "https://github.com/MrEliasen/path-to-power-server/issues"
    },
    "nodemonConfig": {
        "ignore": [
            "node_modules/*",
            "npm-debug.log"
        ]
    },
    "homepage": "https://www.pathtopower.online",
    "dependencies": {
        "bluebird": "^3.5.1",
        "escape-string-regexp": "^1.0.5",
        "event": "^1.0.0",
        "express": "^4.16.2",
        "moment": "^2.20.1",
        "mongoose": "^4.13.7",
        "readline-sync": "^1.4.7",
        "socket.io": "^2.0.4",
        "superagent": "^3.8.2",
        "uuid": "^3.1.0",
        "winston": "^3.0.0-rc1"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-stage-0": "^6.24.1",
        "eslint": "^4.15.0",
        "eslint-config-google": "^0.9.1"
    }
}
